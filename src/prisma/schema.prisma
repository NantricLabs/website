generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_CONNECTOR")
}

model User {
  id       String @id @default(cuid())
  email    String @unique
  password String

  first_name String
  last_name  String

  created_on DateTime @default(now())
  updated_on DateTime @updatedAt
}
