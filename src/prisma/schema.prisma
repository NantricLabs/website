generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_CONNECTOR")
}

model User {
  id       String @id @default(cuid())
  name     String
  email    String @unique
  password String

  created_on DateTime @default(now())
  updated_on DateTime @updatedAt
}
