generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_CONNECTOR")
}

model User {
  id   String @id @default(cuid())
  name String

  email          String  @unique
  emailConfirmed Boolean @default(false)

  password           String
  passwordResetToken String?

  createdOn DateTime @default(now())
  updatedOn DateTime @updatedAt
}
